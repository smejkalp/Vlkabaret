<% form_for @photo, :html => { :multipart => true } do |f| %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :concert_id %><br />
    <%= f.collection_select :concert_id, Concerts.all, :id, :name_date_str %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description, :rows => 10 %>
  </p>
  <p>
    <%= f.file_field :image %>
  </p>
  <p><%= f.submit "UloÅ¾it" %></p>
<% end %>